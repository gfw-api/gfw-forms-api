			"path": "/api/v1/questionnaire/:questionnaireId/answer"
		}
	},{
		"path": "/questionnaire/:questionnaireId/answer/:id",
		"method": "PATCH",
		"authenticated": true,
		"redirect": {
			"method": "PATCH",
			"path": "/api/v1/questionnaire/:questionnaireId/answer/:id"
		}
	},{
		"path": "/questionnaire/:questionnaireId/answer/:id",
		"method": "DELETE",
		"authenticated": true,
		"redirect": {
			"method": "DELETE",
			"path": "/api/v1/questionnaire/:questionnaireId/answer/:id"
		}
